{
  "name": "artemis",
  "version": "0.2.1",
  "description": "A Hugo Boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "gulp clean && run-p hugo:dev gulp:dev",
    "stage": "gulp clean && run-p hugo:stage gulp:prod",
    "production": "gulp clean && run-s hugo:prod gulp:prod",
    "gulp:dev": "gulp --env development",
    "gulp:prod": "gulp --env production",
    "hugo:dev": "hugo -w",
    "hugo:stage": "hugo -w -D -F",
    "hugo:prod": "hugo --minify",
    "clean": "gulp clean",
    "images": "gulp images"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edydeleon/artemis.git"
  },
  "keywords": [
    "hugo",
    "boilerplate"
  ],
  "author": "Edy de Leon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edydeleon/artemis/issues"
  },
  "homepage": "https://github.com/edydeleon/artemis#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.5.0",
    "autoprefixer": "^9.6.0",
    "browser-sync": "^2.26.7",
    "cssnano": "^4.1.10",
    "del": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-environments": "^0.1.2",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^6.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-purgecss": "^1.2.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "npm-run-all": "^4.1.5"
  }
}